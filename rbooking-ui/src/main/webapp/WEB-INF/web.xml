<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>RBooking - UI</display-name>
  <welcome-file-list>
    <welcome-file>/index.html</welcome-file>
  </welcome-file-list>

    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>
            /WEB-INF/spring-configuration/propertyFileConfigurer.xml
        </param-value>
    </context-param>

    <!-- Don't do session timeout in client. (rely on session timeout in teletrol-webservice and teletrol-sso and legacy) -->
    <session-config>
      <session-timeout>-1</session-timeout>
    </session-config>


</web-app>

